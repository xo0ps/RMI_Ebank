<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.iau.sadeghi.ebanking.model.to.ShortProfitableAccountType" table="shortprofitableaccounttype"
           schema="" catalog="ebanking">
        <composite-id mapped="true" class="com.iau.sadeghi.ebanking.model.to.ShortProfitableAccountTypePK">
            <key-property name="row" column="Row"/>
            <key-property name="id" column="ID"/>
        </composite-id>
        <property name="currentMoney" column="CurrentMoney"/>
        <property name="increaseRate" column="IncreaseRate"/>
        <property name="openingDate" column="OpeningDate"/>
        <property name="hasAtmCards" column="HasATMCards"/>
        <property name="atmCardsIDs" column="ATMCardsIDs"/>
    </class>
</hibernate-mapping>